{
  "name": "zfr/zfr-oauth2-server-doctrine",
  "description": "Doctrine 2 adapter for ZfrOAuth2Server",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "oauth 2"
  ],
  "homepage": "http://www.github.com/zf-fr/zfr-oauth2-server-doctrine",
  "authors": [
    {
      "name": "MichaÃ«l Gallego",
      "email": "mic.gallego@gmail.com",
      "homepage": "http://www.michaelgallego.fr"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:basz/zfr-oauth2-server.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "zfr/zfr-oauth2-server": "dev-refactoring-middleware",
    "doctrine/orm": "~2.5",
    "container-interop/container-interop": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.1",
    "squizlabs/php_codesniffer": "^2.5",
    "satooshi/php-coveralls": "~0.6"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "ZfrOAuth2\\Server\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZfrOAuth2Test\\Server\\Doctrine\\": "tests/src/"
    }
  }
